<module name="">
    <bean name="WorldNcProperty">
        <var name="name" type="string"/>
        <var name="value" type="string"/>
    </bean>
    <bean name="WorldNc">
        <var name="name" type="string"/>
        <var name="properties" type="(list#sep=,),WorldNcProperty"/>
    </bean>
    <bean name="World">
        <var name="id" type="UInt32"/>
        <var name="name" type="string"/>
        <var name="nc" type="(list#sep=,),WorldNc"/>
        <var name="nodeType" type="string"/>
    </bean>

    <table name="WorldData" mode="list" value="World" input="*@World.json"/>

</module>