<module name="">
    <bean name="SceneTemplateNcProperty">
        <var name="name" type="string"/>
        <var name="value" type="string"/>
    </bean>
    <bean name="SceneTemplateNc">
        <var name="name" type="string"/>
        <var name="properties" type="(list#sep=,),SceneTemplateNcProperty"/>
    </bean>
    <bean name="SceneTemplate">
        <var name="id" type="UInt32"/>
        <var name="name" type="string"/>
        <var name="nc" type="(list#sep=,),SceneTemplateNc"/>
        <var name="nodeType" type="string"/>
        <var name="specType" type="UInt32"/>
    </bean>

    <table name="SceneTemplateData" mode="list" value="SceneTemplate" input="*@SceneTemplate.json"/>

</module>